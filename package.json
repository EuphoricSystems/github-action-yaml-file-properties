{
  "name": "github-action-yaml-file-properties",
  "version": "1.1.1",
  "author": "EuphoricSystems",
  "description": "GitHub Action to load properties from a YAML file.",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint -c .eslintrc.json src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EuphoricSystems/github-action-yaml-file-properties"
  },
  "keywords": [
    "actions",
    "typescript",
    "GitHub"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/exec": "1.1.1",
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.7",
    "@types/semver": "7.3.9",
    "@typescript-eslint/parser": "5.26.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.16.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.2.2",
    "jest": "28.1.0",
    "jest-circus": "28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.3",
    "typescript": "4.7.2"
  }
}
